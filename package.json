{
  "name": "sounddocs",
  "private": true,
  "version": "1.5.6.6",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "agents/*",
    "models/*",
    "tools/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @sounddocs/web dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "preview": "pnpm --filter @sounddocs/web preview",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "3.5.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
